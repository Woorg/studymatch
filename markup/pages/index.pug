doctype html
html(class="no-js", lang="ru")

    include ../blocks/head/head.pug
    include ../blocks/header/header.pug
    
    include ../blocks/hero/hero.pug
    include ../blocks/doing/doing.pug
    include ../blocks/events/events.pug
    include ../blocks/service/service.pug
    include ../blocks/online/online.pug
    include ../blocks/markets/markets.pug
    include ../blocks/cta/cta.pug
    include ../blocks/why/why.pug
    include ../blocks/attraction/attraction.pug
    include ../blocks/examples/examples.pug
    include ../blocks/cta/cta.pug
    include ../blocks/reviews/reviews.pug
    include ../blocks/cost/cost.pug
    include ../blocks/partners/partners.pug
    include ../blocks/map/map.pug
    include ../blocks/popup/popup.pug

    include ../blocks/footer/footer.pug

    head
        +head(head.defaults)
    body.page
        .page__wrapper
            |%=symbols=%
            +header()
            main.page__inner
                +hero()#hero
                +doing()#doing
                +events()#events
                +service()#service
                +online()#online
                +markets()#markets
                +cta()#cta-first
                +why()#why
                +attraction()#attraction
                +examples()#examples
                +cta()
                +reviews()#reviews
                +cost()#cost
                +partners()#partners
                +cta()
                +map()#map-section

            .page__footer
                +footer()
                +popup()


        script.
            var map;
            function initMap() {
                map = new google.maps.Map(document.getElementById('map'), {
                    center: {lat: 39.757925, lng: -75.606907},
                    zoom: 17,
                    disableDefaultUI: true

                });
                var marker = new google.maps.Marker({
                    position: {lat: 39.757925, lng: -75.606907},
                    map: map,
                    title: '1013 Centre Road, Suite 403-B, Wilmington, DE 19805, New Castle.'
                });
            }

        script(src="//maps.googleapis.com/maps/api/js?key=AIzaSyDDO60TDSumlziHzbybxGGbAEegP7zLX0Y&callback=initMap" async defer)
        script(src='%=static=%js/separate-js/jquery.min.js')
        script(src='%=static=%js/separate-js/slick.min.js')
        script(src='%=static=%js/separate-js/jquery.magnific-popup.min.js')

        script(src='%=static=%js/main%=hash=%%=min=%.js')



